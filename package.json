{
  "name": "angular-tailwind-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4300",
    "start:dev": "ng serve --port 4300 --host 0.0.0.0",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e:visual": "node e2e/run-visual-tests.js",
    "e2e:visual:headless": "HEADLESS=true node e2e/run-visual-tests.js",
    "e2e:visual:simple": "npx ts-node --project e2e/tsconfig.json e2e/simple-test.ts",
    "e2e:visual:report": "npm run e2e:visual && open e2e/visual-test-report.html"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.7",
    "@angular/cli": "^19.2.7",
    "@angular/compiler-cli": "^19.2.0",
    "@tailwindcss/postcss": "^4.1.10",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^30.0.0",
    "@types/node-fetch": "^2.6.12",
    "@types/selenium-webdriver": "^4.1.28",
    "autoprefixer": "^10.4.21",
    "chromedriver": "^138.0.1",
    "geckodriver": "^5.0.0",
    "jasmine-core": "~5.6.0",
    "jest": "^29.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "selenium-webdriver": "^4.34.0",
    "tailwindcss": "^4.1.10",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2"
  }
}
